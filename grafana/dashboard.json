        {
          "datasource": {
            "type": "postgres",
            "uid": "postgres"
          },
          "rawSql": "SELECT\n  CASE\n    WHEN (completed + failed) = 0 THEN 0\n    ELSE ROUND(completed::numeric / (completed + failed), 4)\n  END as value\nFROM (\n  SELECT\n    COUNT(*) FILTER (WHERE status = 'completed' AND completed_at >= $__timeFrom() AND completed_at <= $__timeTo()) as completed,\n    COUNT(*) FILTER (WHERE status IN ('failed', 'dead') AND completed_at >= $__timeFrom() AND completed_at <= $__timeTo()) as failed\n  FROM jobs\n) counts",
          "format": "table",
          "refId": "A"
        }
      ],
      "title": "Success Rate",
      "type": "stat"
